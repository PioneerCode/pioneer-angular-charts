{
  "name": "pioneer-charts",
  "description": "Pioneer Charts, a library of data visualization charts build on top of Angular and D3.",
  "homepage": "https://charts.pioneercode.com",
  "bugs": {
    "url": "https://github.com/PioneerCode/pioneer-charts/issues"
  },
  "author": {
    "name": "Chad Ramos",
    "email": "hello@pioneercode.com",
    "url": "https://pioneercode.com"
  },
  "keywords": [
    "pioneer code",
    "pioneer charts",
    "data visualization",
    "charts",
    "d3",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PioneerCode/pioneer-charts"
  },
  "version": "0.6.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --app=dev",
    "build:dev": "npm run build:lib && ng build --aot --app=dev",
    "start:docs": "ng serve --app=docs",
    "build:docs": "ng build --prod --app=docs",
    "build:docs:404": "node ./build/404.js",
    "build:docs:prod": "npm run build:lib && npm run build:docs && npm run build:docs:404",
    "build:lib": "ng-packagr -p libs/pcac/package.json && npm run build:lib:sass && npm run build:lib:theme  && npm run build:lib:readme",
    "build:lib:publish": "npm run build:lib && npm publish dist/bundles/@pioneer-code/pioneer-charts --access public",
    "build:lib:readme": "node ./build/readme.js",
    "build:lib:theme": "node ./build/theme.js",
    "build:lib:sass": "node ./build/sass.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "npm": "^5.8.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@pioneer-code/pioneer-charts": "^0.6.0",
    "@ryancavanaugh/prismjs": "^1.4.3-alpha",
    "@types/d3": "^5.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "bootstrap": "^4.0.0",
    "chalk": "^2.3.2",
    "codelyzer": "^4.0.1",
    "copyfiles": "^2.0.0",
    "cpy": "^6.0.0",
    "d3": "^4.13.0",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "node-sass": "^4.8.3",
    "prismjs": "^1.12.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
