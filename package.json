{
  "name": "pioneer-charts",
  "version": "0.11.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve pioneer-charts-dev",
    "start:docs": "ng serve pioneer-charts --port 4201",
    "build": "ng build",
    "build:docs": "ng build pioneer-charts --prod",
    "build:docs:404": "node ./build/404.js",
    "build:docs:prod": "npm run build:lib && npm run build:docs && npm run build:docs:404",
    "build:lib": "npm run build:lib:package && npm run build:lib:sass && npm run build:lib:theme  && npm run build:lib:readme",
    "build:lib:publish": "npm run build:lib && npm publish dist/pcac --access public",
    "build:lib:publish:tag": "npm run build:lib && npm publish dist/pcac --access public --tag rc",
    "build:lib:readme": "node ./build/readme.js",
    "build:lib:package": "ng build pcac",
    "build:lib:theme": "node ./build/theme.js",
    "build:lib:sass": "node ./build/sass.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/animations": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@pioneer-code/pioneer-charts": "^0.11.2",
    "@ryancavanaugh/prismjs": "^1.4.3-alpha",
    "@types/d3": "^5.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.1.1",
    "chalk": "^2.4.1",
    "codelyzer": "~4.2.1",
    "copyfiles": "^2.0.0",
    "cpy": "^6.0.0",
    "d3": "^5.1.0",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "prismjs": "^1.14.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
